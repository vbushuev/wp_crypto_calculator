// out: ../css/style.css, compressed: false

.-cc-container
    position: relative;
    font-family: "Open Sans",sans-serif;
    font-size: 14px;
    .panel-calculator
        background: #f5f5f5;
        border-color: #DDD;
        .panel-image
            background: #FFF;
            text-align: center;
            padding: 5px 0;
            border-radius: 4px 4px 0 0;
            img
                width: 40%;
                max-width: 150px;
        .calculated-for
            padding: 3px;
            background: #EEEEEE;
            margin-bottom: 10px;
            border: 1px solid #DDD;
            border-top: 0;
            font-size: 12px;
            text-align: center;
            border-left: 0;
            border-right: 0;
            color: #777;
            box-shadow: inset 1px 4px 9px -6px;
            line-height: 18px;
        .panel-body
            padding: 15px;
            label
                display: inline-block;
                max-width: 100%;
                margin-bottom: 5px;
                font-weight: 700;
                position: relative;
            input,select
                background: #FFF;
                font-weight: 100;
                color:#555;
                padding-left: 10px;
                font-size: 14px;
                border: solid 1px #ddd;
                height:30px;
                line-height: 30px;
                min-width: 120px;
            .input-group
                float:none;
    .results-positive
        .results-header
            overflow: hidden;
            margin-bottom: 15px;
            margin-top: 26px;
            .circle-values
                margin-bottom: 5px;
                overflow: hidden;
                text-align: center;
                .circle-value
                    margin-left: 10px;
                    display: block;
                    float: right;
                    color: #FFFFFF;
                    width: 45%;
                    text-align: right;
                    background: #FFE0E0;
                    padding: 10px 10px;
                    border-bottom: 2px solid #E29090;
                    border-right: 1px solid #E29090;
                    border-radius: 10px;
                    padding-left: 10px;
                    .circle-label
                        display: inline-block;
                        line-height: 25px;
                        font-weight: bold;
                        font-size: 12px;
                        background: #FFF;
                        color: #B13737;
                        padding: 0 10px;
                        border-radius: 10px;
                        text-transform: uppercase;
                        -webkit-box-shadow: 1px 2px 1px 0px rgba(0,0,0,0.3);
                        -moz-box-shadow: 1px 2px 1px 0px rgba(0,0,0,0.3);
                        box-shadow: 1px 2px 1px 0px rgba(0,0,0,0.3);
                    .circle-content
                        font-size: 28px;
                        font-weight: bold;
                        color: #258C66;
                        text-shadow: 2px 4px 3px rgba(0,0,0,0.1);
                .circle-first
                    background: #C4E8DB;
        .calculator-container
            position: relative;
            border: none;//solid 1px rgba(0,0,0,.5);
            border-radius: 10px;
            margin-bottom: 15px;
            position: relative;
            height: 70px;
            line-height: 20px;
            .calculator-row-name
                font-weight: 100;
                position: absolute;
                left: 0;
                bottom: -2px;
                width: 55px;
                padding-left: 10px;
                background: #258C66;

                color: #FFF;
                border-radius: 0 10px 0 10px;
                border: 2px solid #258C66;
                border-right: 0;
                font-size: 10px;
                font-weight: bold;
            .calculator-col
                float: left;
                width: 33.3%;
                padding: 10px 10px;
                border-right: 1px solid #D4D4D4;
                border-top: 1px solid #D4D4D4;
                border-bottom: 2px solid #D4D4D4;
                text-align: right;
                .calculator-label
                    color: rgba(0,0,0,.5);
                    // color: rgba(255,255,255,1);
                    font-size: 12px;
                    font-weight: 100;
                .calculator-value
                    font-weight: 600;
                    font-size: 20px;
                    line-height: 30px;
            .calculator-first-col
                background: #3FB488;
                border-color: #258C66;
                color: #FFF;
                border-radius: 10px 0 0 10px;
                .calculator-label
                    // color: rgba(0,0,0,.5);
                    color: rgba(255,255,255,1);
                    font-size: 12px;
                    font-weight: 100;
            .calculator-col:last-of-type
                border-radius: 0 10px 10px 0;
